{
    "presets": [
      ["env", {
        "modules": false,
        "targets": {
          "browsers": ["> 0.5%", "last 2 versions", "not ie <= 8", "Android > 4.1"]
        }
      }],
      "stage-2"
    ],
    "plugins": ["transform-vue-jsx", "transform-runtime", "syntax-dynamic-import"],
    "env": {
      "test": {
        "presets": ["env", "stage-2"],
